<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>PrabinWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>addEmp</servlet-name>
    <servlet-class>controller.EmpServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>addEmp</servlet-name>
    <url-pattern>/addEmpUrl</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>delUpdEmp</servlet-name>
    <servlet-class>controller.EmpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>delUpdEmp</servlet-name>
    <url-pattern>/actUrl</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>editEmp</servlet-name>
    <servlet-class>controller.EditEmpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>editEmp</servlet-name>
    <url-pattern>/editEmpUrl</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>searchEmp</servlet-name>
    <servlet-class>controller.SearchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>searchEmp</servlet-name>
    <url-pattern>/searchEmpUrl</url-pattern>
  </servlet-mapping>
</web-app>